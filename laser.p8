--main functions--

function _init()
	scene=1
	
	game_init()
	select_init()
	menu_init()
end


function _update()
	
	if scene==1 then
		menu_update()
	elseif scene==2 then
		select_update()
	elseif scene==3 then
		game_update()
	end

end


function _draw()
	
	if scene==1 then
		menu_draw()
	elseif scene==2 then
		select_draw()
	elseif scene==3 then
		game_draw()
	end

end
