--gameplay functions--

function game_init()
	laserdown=13
	laserup=14
	laserside=15
	laserspd=3.4
	mspd=1
	
	wave_timer=0
	wave1_begin=20 wave1_end=400
	wave2_end=800
	wave3_end=1400
	wave4_end=1700
	wave5_end=2100
	
	wave2_begin=wave1_end
	wave3_begin=wave2_end
	wave4_begin=wave3_end
	wave5_begin=wave4_end
	wave6_begin=wave5_end
	
	game_over=false
	timer=0
	
	p1={}
	p2={}
	
	p1.sp=1
	p1.x=43
	p1.y=30
	p1.sy=30
	p1.hflip=false
	p1.spd=1.5
	p1.atimer=0
	p1.dead=false
	p1.jump=false
	p1.jumpsp=60
	p1.jump_vel=-0.5
	p1.move=true
	p1.deadtimer=5
	p1.deadcounter=1
	
	p2.sp=7
	p2.x=83
	p2.y=30
	p2.sy=30
	p2.hflip=false
	p2.spd=1.5
	p2.atimer=0
	p2.dead=false
	p2.jump=false
	p2.jumpsp=60
	p2.jump_vel=-0.5
	p2.move=true
	p2.deadtimer=5
	p2.deadcounter=1
	
	orb1={dead=false,sp1=12,sp2=14,x1=61,y1=152,x2=61,y2=-31,timer=0,spd=laserspd,mspd=1}
	orb2={dead=false,sp1=15,sp2=15,x1=152,y1=61,x2=-31,y2=61,timer=0,spd=laserspd,mspd=1}
	orb3={dead=false,sp1=12,sp2=14,x1=61,y1=152,x2=61,y2=-31,timer=0,spd=laserspd,mspd=1}

	orb4={dead=false,sp1=12,sp2=14,x1=61,y1=152,x2=61,y2=-31,timer=0,spd=laserspd,mspd=1}
	orb5={dead=false,sp1=15,sp2=15,x1=152,y1=61,x2=-31,y2=61,timer=0,spd=laserspd,mspd=1}
	orb6={dead=false,sp1=12,sp2=14,x1=61,y1=152,x2=61,y2=-31,timer=0,spd=laserspd,mspd=1}

	
	wave=1
	
end


function game_update()
	if (game_over==false) timer+=0.033333
	
	if p1.jump==true then		
		if p1.jump_vel>=0.5 then
			p1.jump_vel=0
			p1.jump=false
		else
			p1.y+=p1.jump_vel*2
			p1.jump_vel+=0.02		
		end
		
		if (p1.jump_vel<0) p1.jumpsp=63
		if (p1.jump_vel>0.2) p1.jumpsp=62
		if (p1.jump==false) p1.jumpsp=61
	end
	
	if p2.jump==true then		
		if p2.jump_vel>=0.5 then
			p2.jump_vel=0
			p2.jump=false
		else
			p2.y+=p2.jump_vel*2
			p2.jump_vel+=0.02		
		end
		
		if (p2.jump_vel<0) p2.jumpsp=63
		if (p2.jump_vel>0.2) p2.jumpsp=62
		if (p2.jump==false) p2.jumpsp=61
	end

	if p1.dead then
		p1.sp=2
		p1.move=false
		p1.deadtimer-=0.03333
	else
		p1.move=true
	end
	
	if p2.dead then
		p2.sp=8
		p2.move=false
		p2.deadtimer-=0.03333
	else
		p2.move=true
	end
	
	if (p1.deadtimer<=0 and game_over==false) p1.dead=false p1.deadcounter+=1 p1.deadtimer=5*p1.deadcounter
	if (p2.deadtimer<=0 and game_over==false) p2.dead=false p2.deadcounter+=1 p2.deadtimer=5*p2.deadcounter

	if (p1.x>orb1.x1 and p1.x<orb1.x1+4 and p1.jump==false and orb1.dead) p1.dead=true	
	if (p1.y>orb2.y1-4 and p1.y<orb2.y1 and p1.jump==false and orb2.dead) p1.dead=true
	if (p1.x>orb3.x1 and p1.x<orb3.x1+4 and p1.jump==false and orb3.dead) p1.dead=true
	if (p1.x>orb4.x1 and p1.x<orb4.x1+4 and p1.jump==false and orb4.dead) p1.dead=true	
	if (p1.y>orb5.y1-4 and p1.y<orb5.y1 and p1.jump==false and orb5.dead) p1.dead=true
	if (p1.x>orb6.x1 and p1.x<orb6.x1+4 and p1.jump==false and orb6.dead) p1.dead=true

	if (p2.x>orb1.x1 and p2.x<orb1.x1+4 and p2.jump==false and orb1.dead) p2.dead=true	
	if (p2.y>orb2.y1-4 and p2.y<orb2.y1 and p2.jump==false and orb2.dead) p2.dead=true
	if (p2.x>orb3.x1 and p2.x<orb3.x1+4 and p2.jump==false and orb3.dead) p2.dead=true
	if (p2.x>orb4.x1 and p2.x<orb4.x1+4 and p2.jump==false and orb4.dead) p2.dead=true	
	if (p2.y>orb5.y1-4 and p2.y<orb5.y1 and p2.jump==false and orb5.dead) p2.dead=true
	if (p2.x>orb6.x1 and p2.x<orb6.x1+4 and p2.jump==false and orb6.dead) p2.dead=true

	
	if (p1.dead and p2.dead) game_over=true
	
	wave_timer+=1
	mspd+=0.0001
	
	player_movement()

end


function game_draw()
	
	cls()
	map()
	
	if (wave_timer>wave1_begin) orb2_spawn()	
	if (wave_timer>wave2_begin) orb1_spawn()	
	if (wave_timer>wave3_begin) orb3_spawn()
	if (wave_timer>wave4_begin) orb4_spawn()	
	if (wave_timer>wave5_begin) orb5_spawn()	
	if (wave_timer>wave6_begin) orb6_spawn()
	
	if p1_ready==true then
		spr(p1.jumpsp,p1.x,p1.sy)
		spr(p1.sp,p1.x,p1.y,1,1,p1.hflip)
		
		if p1.dead and game_over==false then
			print(ceil(p1.deadtimer),p1.x+2,p1.y-5,7)
		end
		
	else
		p1.dead=true
	end
	
	if p2_ready==true then
		spr(p2.jumpsp,p2.x,p2.sy)
		spr(p2.sp,p2.x,p2.y,1,1,p2.hflip)
	
		if p2.dead and game_over==false then
			print(ceil(p2.deadtimer),p2.x+1,p2.y-5,7)
		end
	
	else
		p2.dead=true
	end
	
	if game_over then
		print("game over!",45,63,7)
	end
	
	print(ceil(timer),3,120,7)

end


function player_movement()
	
	if p1.move then
		if (btn(‚¨ÖÔ∏è)) p1.x-=p1.spd p1.sp=3 p1.hflip=true
		if (btn(‚û°Ô∏è)) p1.x+=p1.spd p1.sp=3 p1.hflip=false
		if (btn(‚¨ÜÔ∏è)) p1.y-=p1.spd p1.sy-=p1.spd p1.sp=5 p1.hflip=false
		if (btn(‚¨áÔ∏è)) p1.y+=p1.spd p1.sy+=p1.spd p1.sp=1 p1.hflip=false
		if (btnp(üÖæÔ∏è) and p1.jump==false) p1.jump=true p1.jump_vel=-0.5 sfx(2)
	end
	
	if p2.move then
		if (btn(‚¨ÖÔ∏è,1)) p2.x-=p2.spd p2.sp=9 p2.hflip=true
		if (btn(‚û°Ô∏è,1)) p2.x+=p2.spd p2.sp=9 p2.hflip=false
		if (btn(‚¨ÜÔ∏è,1)) p2.y-=p2.spd p2.sy-=p2.spd p2.sp=11 p2.hflip=false
		if (btn(‚¨áÔ∏è,1)) p2.y+=p2.spd p2.sy+=p2.spd p2.sp=7 p2.hflip=false
		if (btnp(üÖæÔ∏è,1) and p2.jump==false) p2.jump=true p2.jump_vel=-0.5 sfx(2)
	end

	if (p1.x>=121) p1.x=121
	if (p1.x<=-1) p1.x=-1
	if (p1.y<=0) p1.y=0 p1.sy=2
	if (p1.y>=120) p1.y=120 p1.sy=122
	
	if (p2.x>=121) p2.x=121
	if (p2.x<=-1) p2.x=-1
	if (p2.y<=0) p2.y=0 p2.sy=2
	if (p2.y>=120) p2.y=120 p2.sy=122

end


function orb1_spawn()
	spr(laserup,orb1.x1,orb1.y1)
	spr(laserdown,orb1.x2,orb1.y2)
	
	orb1.y1-=orb1.spd
	orb1.y2+=orb1.spd	
	orb1.spd*=0.9
	
	if orb1.timer>60 then
		orb1.sp1=13
	end
	
	if orb1.timer>70 then
		sfx(3)
		orb1.dead=true
		line(orb1.x1+3,orb1.y1-1,orb1.x2+3,orb1.y2+7,8)
		orb1.x1-=mspd*orb1.mspd
		orb1.x2-=mspd*orb1.mspd
	end
	
	if (orb1.x1>=120) orb1.mspd*=-1
	if (orb1.x1<=0) orb1.mspd*=-1
	
	orb1.timer+=1
end


